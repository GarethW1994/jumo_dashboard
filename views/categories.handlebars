
{{>header}}

<div class="container">

{{#each importantData}}
 <div class="info">
	<h5>Name: {{userName}}</h5>  <h5>Surname: {{userSurname}}</h5> 	<h5>City: {{userCity}}</h5><br>
	 <h5>About {{userName}}:</h5>  
	 <p>{{userAbout}}</p>
	 <hr/>
</div>

{{/each}}
  
 <div class="info2">
	
  <div class="row1">
    <div class="nine.columns" class="important">
      <h4>Important Expenses</h4>
      <table>
          <tr>
            <th>Category Name</th>
            <th>Amount</th>
            <th>Rating</th>
          </tr>

			  {{#each importantData}}
			  {{#each important}}
          <tr>
			  		<td>{{category}}</td>
			  		<td>{{amount}}</td>
			  		<td>{{rating}}</td>
		 </tr>
			  {{/each}}
			  {{/each}}
      </table>
    </div>
	  <br>

  </div><br>

  <div class="row2">
    <div class="nine.columns" class="neutral">
	  <h4>Neutral Expenses</h4>
      <table>
          <tr>
            <th>Category Name</th>
            <th>Amount</th>
            <th>Rating</th>
          </tr>

			  {{#each importantData}}
			  {{#each neutral}}
          <tr>
			  		<td>{{category}}</td>
			  		<td>{{amount}}</td>
			  		<td>{{rating}}</td>
		 </tr>
			  {{/each}}
			  {{/each}}
      </table>
	  </div><br>
  </div><br>

  <div class="row3">
    <div class="nine.columns" class="notImportant">
	  <h4>Not Important Expenses</h4>
      <table>
          <tr>
            <th>Category Name</th>
            <th>Amount</th>
            <th>Rating</th>
          </tr>

			  {{#each importantData}}
			  {{#each notImportant}}
          <tr>
			  		<td>{{category}}</td>
			  		<td>{{amount}}</td>
			  		<td>{{rating}}</td>
		 </tr>
			  {{/each}}
			  {{/each}}
      </table>
	  </div>
  </div>


</div>
</div>
{{>footer}}